            "RichText": {
                "properties": {
                    "text": { "type": "string", "title": "Text" },
                    "type": { "type": "string", "title": "Type" },
                    "bold": { "type": "boolean", "title": "Bold", "default": false },
                    "italic": { "type": "boolean", "title": "Italic", "default": false },
                    "strikethrough": {
                        "type": "boolean",
