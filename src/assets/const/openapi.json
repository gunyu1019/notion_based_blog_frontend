                    "text": {
                        "items": { "$ref": "#/components/schemas/RichText" },
                        "type": "array",
                        "title": "Text"
                    },
                    "captions": {
                        "items": { "$ref": "#/components/schemas/RichText" },
                        "type": "array",
                        "title": "Captions"
                    },
                    "is_file_available": {
                        "type": "boolean",
                        "title": "Is File Available",
                        "default": false
                    },
                    "file": { "$ref": "#/components/schemas/File" }
                },
                "type": "object",
                "required": ["id", "type", "file"],
                "title": "FileBlock"
            },
            "HTTPValidationError": {
                "properties": {
                    "detail": {
                        "items": { "$ref": "#/components/schemas/ValidationError" },
                        "type": "array",
                        "title": "Detail"
                    }
                },
                "type": "object",
                "title": "HTTPValidationError"
            },
            "PostItem": {
                "properties": {
                    "id": { "type": "string", "format": "uuid", "title": "Id" },
                    "title": { "type": "string", "title": "Title" },
