{
    "openapi": "3.1.0",
    "info": { "title": "FastAPI", "version": "0.1.0" },
    "paths": {
        "/content": {
            "get": {
                "summary": "Content",
                "operationId": "content_content_get",
                "parameters": [
                    {
                        "name": "item_id",
                        "in": "query",
                        "required": true,
                        "schema": { "type": "string", "format": "uuid", "title": "Item Id" }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": { "$ref": "#/components/schemas/Content" },
                                    "title": "Response Content Content Get"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
                            }
                        }
                    }
                }
            }
        },
        "/posts": {
            "get": {
                "summary": "List Of Posts",
                "operationId": "list_of_posts_posts_get",
                "parameters": [
                    {
                        "name": "private_access",
                        "in": "query",
                        "required": false,
                        "schema": { "type": "boolean", "default": false, "title": "Private Access" }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": { "$ref": "#/components/schemas/PostItem" },
                                    "title": "Response List Of Posts Posts Get"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
                            }
                        }
                    }
                }
            }
        },
        "/post": {
            "get": {
                "summary": "Post Info",
                "operationId": "post_info_post_get",
                "parameters": [
                    {
                        "name": "post_id",
                        "in": "query",
                        "required": true,
                        "schema": { "type": "string", "format": "uuid", "title": "Post Id" }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": { "$ref": "#/components/schemas/PostItemDetail" }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "Block": {
                "properties": {
                    "id": { "type": "string", "format": "uuid", "title": "Id" },
                    "type": { "type": "string", "title": "Type" },
                    "has_children": {
                        "type": "boolean",
                        "title": "Has Children",
                        "default": false
                    },
                    "children": {
                        "items": {
                            "anyOf": [
                                { "$ref": "#/components/schemas/CodeBlock" },
                                { "$ref": "#/components/schemas/TableBlock" },
                                { "$ref": "#/components/schemas/UrlBlock" },
                                { "$ref": "#/components/schemas/Block" },
                                { "$ref": "#/components/schemas/FileBlock" }
                            ]
                        },
                        "type": "array",
                        "title": "Children"
                    },
                    "text": {
                        "items": { "$ref": "#/components/schemas/RichText" },
                        "type": "array",
                        "title": "Text"
                    },
                    "captions": {
                        "items": { "$ref": "#/components/schemas/RichText" },
                        "type": "array",
                        "title": "Captions"
                    },
                    "is_file_available": {
                        "type": "boolean",
                        "title": "Is File Available",
                        "default": false
                    }
                },
                "type": "object",
                "required": ["id", "type"],
                "title": "Block"
            },
            "Category": {
