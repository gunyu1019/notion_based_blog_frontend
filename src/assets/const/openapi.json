                    "has_children": {
                        "type": "boolean",
                        "title": "Has Children",
                        "default": false
                    },
                    "children": {
                        "items": {
                            "anyOf": [
                                { "$ref": "#/components/schemas/CodeBlock" },
                                { "$ref": "#/components/schemas/TableBlock" },
