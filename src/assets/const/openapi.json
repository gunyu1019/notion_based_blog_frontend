                        "title": "Expiry Time"
                    }
                },
                "type": "object",
                "required": ["extra_key", "url", "expiry_time"],
                "title": "Content"
            },
            "File": {
                "properties": {
                    "type": { "type": "string", "title": "Type" },
                    "url": { "type": "string", "title": "Url", "readOnly": true },
                    "expiry_time": {
                        "anyOf": [{ "type": "string", "format": "date-time" }, { "type": "null" }],
                        "title": "Expiry Time",
                        "readOnly": true
                    }
                },
                "type": "object",
                "required": ["type", "url", "expiry_time"],
                "title": "File"
            },
            "FileBlock": {
                "properties": {
                    "id": { "type": "string", "format": "uuid", "title": "Id" },
                    "type": { "type": "string", "const": "file", "title": "Type" },
                    "has_children": {
                        "type": "boolean",
                        "title": "Has Children",
                        "default": false
                    },
                    "children": {
                        "items": {
                            "anyOf": [
                                { "$ref": "#/components/schemas/CodeBlock" },
